  "scripts": {
    "migration:generate": "npx sequelize-cli migration:generate --migrations-path src/database/migrations --name",
    "migration:undo": "npx sequelize-cli db:migrate:undo --migrations-path dist/database/migrations",
    "migrate": "npx sequelize db:migrate",
    "seed": "npx sequelize db:seed:all",
    "build": "tsc",
    "watch": "tsc -w",
    "pretest": "NODE_ENV=test sequelize db:migrate && NODE_ENV=test sequelize db:seed:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --files src/server.ts",    
    "rebuild": "npm run build && npm start"
  },
